{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/dist"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://cloud.appwrite.io/v1/:path*"
      }
    ]
  }
  